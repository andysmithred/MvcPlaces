<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - WebApplication2</title>
    <environment include="Development">
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
        <link rel="stylesheet" href="~/css/site.css" />
        <link rel="stylesheet" href="~/css/MvcGrid/mvc-grid.css">
        <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.5.0/css/all.css" integrity="sha384-B4dIYHKNBt8Bc12p+WXckhzcICo0wtJAoU8YZTY5qE0Id1GSseTk6S+L3BlXeVIU" crossorigin="anonymous">
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
        <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/css/MvcGrid/mvc-grid.css">
        <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.5.0/css/all.css" integrity="sha384-B4dIYHKNBt8Bc12p+WXckhzcICo0wtJAoU8YZTY5qE0Id1GSseTk6S+L3BlXeVIU" crossorigin="anonymous">
    </environment>
</head>
<body class="bg-dark">
    <div class="d-flex">
        <div class="bg-secondary">
            <div class="icon-bar p-0 ml-0" style="height:100%">
                <a asp-controller="Home" asp-action="Index"><i class="fa fa-home"></i></a>
                <a asp-controller="Continent" asp-action="Index"><i class="fas fa-globe-africa"></i></a>
                <a asp-controller="TerritoryType" asp-action="Index"><i class="far fa-map"></i></a>
                <a asp-controller="Territory" asp-action="Index"><i class="fas fa-atlas"></i></a>
                <a asp-controller="Place" asp-action="Index"><i class="fas fa-map-marker-alt"></i></a>
                <a asp-controller="Flag" asp-action="Index"><i class="far fa-flag"></i></a>
                <a asp-controller="PlaceGroup" asp-action="Index"><i class="fas fa-object-group"></i></a>
                <a asp-controller="Drive" asp-action="Index"><i class="fas fa-road"></i></a>
                <a asp-controller="Route" asp-action="Index"><i class="fas fa-plane"></i></a>
                <a asp-controller="Home" asp-action="Contact"><i class="fa fa-envelope"></i></a>
            </div>
        </div>
        <div class="bg-dark">
            <div class="sidenav">
                <a class="nav-link" asp-controller="Home" asp-action="Index">Home</a>
                <button class="dropdown-btn">
                    Lists
                    <i class="fa fa-caret-down"></i>
                </button>
                <div class="dropdown-container">
                    <a asp-controller="Continent" asp-action="Index">Continents</a>
                    <a asp-controller="TerritoryType" asp-action="Index">Territory Types</a>
                    <a asp-controller="Territory" asp-action="Index">Territories</a>
                    <a asp-controller="Place" asp-action="Index">Places</a>
                    <a asp-controller="PlaceGroup" asp-action="Index">Place Groups</a>
                    <a asp-controller="PlaceGroupSet" asp-action="Index">Place Group Sets</a>
                    <a asp-controller="TerritoryPlace" asp-action="Index">Territory Places</a>
                    <a asp-controller="Flag" asp-action="Index">Flags</a>
                    <a asp-controller="Drive" asp-action="Index">Drives</a>
                    <a asp-controller="DriveLeg" asp-action="Index">Drive Legs</a>
                    <a asp-controller="Route" asp-action="Index">Routes</a>
                </div>
                <button class="dropdown-btn">
                    Imports
                    <i class="fa fa-caret-down"></i>
                </button>
                <div class="dropdown-container">
                    <a asp-controller="ImportContinent" asp-action="Index">Continents</a>
                    <a asp-controller="ImportCountry" asp-action="Index">Countries</a>
                    <a asp-controller="ImportRegion" asp-action="Index">Regions</a>
                    <a asp-controller="ImportPlace" asp-action="Index">Places</a>
                    <a asp-controller="ImportPlaceGroup" asp-action="Index">Place Groups</a>
                    <a asp-controller="ImportPlaceGroupSet" asp-action="Index">Place Group Sets</a>
                    <a asp-controller="ImportAirfield" asp-action="Index">Airfields</a>
                    <a asp-controller="ImportDrive" asp-action="Index">Drives</a>
                    <a asp-controller="ImportDriveLeg" asp-action="Index">Drive Legs</a>
                </div>
                <a class="nav-link" asp-controller="Home" asp-action="About">About</a>
                <a class="nav-link" asp-controller="Home" asp-action="Contact">Contact</a>
            </div>
        </div>
        <div class="bg-dark flex-grow-1 p-3">
            @RenderBody()
        </div>
    </div>
   
    <partial name="_CookieConsentPartial" />

    <environment include="Development">
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
        <script src="~/js/mvc-grid.js"></script>
        <script>
            $('.mvc-grid').mvcgrid();
        </script>
    </environment>
    <environment exclude="Development">
        <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-3.3.1.min.js"
                asp-fallback-src="~/lib/jquery/dist/jquery.min.js"
                asp-fallback-test="window.jQuery"
                crossorigin="anonymous"
                integrity="sha384-tsQFqpEReu7ZLhBV2VZlAu7zcOV+rXbYlF2cqB8txI/8aZajjp4Bqd+V6D5IgvKT">
        </script>
        <script src="~/js/mvc-grid.js"></script>
        <script>
            $('.mvc-grid').mvcgrid();
        </script>
        <script src="https://ajax.aspnetcdn.com/ajax/bootstrap/4.1.3/bootstrap.min.js"
                asp-fallback-src="~/lib/bootstrap/dist/js/bootstrap.min.js"
                asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal"
                crossorigin="anonymous"
                integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa">
        </script>
        <script src="~/js/site.min.js" asp-append-version="true"></script>
    </environment>

    @RenderSection("Scripts", required: false)
</body>
</html>