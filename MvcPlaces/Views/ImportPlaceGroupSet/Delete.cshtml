@model MvcPlaces.ModelsImport.PlaceGroupSet

@{
    ViewData["Title"] = "Delete";
}

<br />
<partial name="DeleteImportMenu" />
<br />

<div class="card rounded-0">
    <div class="card-header bg-dark text-white rounded-0">Place Group Set</div>
    <div class="card-body bg-light text-dark">
        <h3>Are you sure you want to delete this?</h3>
        <hr />
        <div class="d-flex">
            <div class="w-25 p-1 pr-4 mb-1 bg-secondary text-white text-right"><label asp-for="Id"></label></div>
            <div class="w-75 p-1 pl-4 mb-1 bg-white"><span asp-for="Id">@Model.Id</span></div>
        </div>
        <div class="d-flex">
            <div class="w-25 p-1 pr-4 mb-1 bg-secondary text-white text-right"><label asp-for="Place.Name"></label></div>
            <div class="w-75 p-1 pl-4 mb-1 bg-white">@Model.Place.Name</div>
        </div>
        <div class="d-flex">
            <div class="w-25 p-1 pr-4 mb-1 bg-secondary text-white text-right"><label asp-for="Group.Name"></label></div>
            <div class="w-75 p-1 pl-4 mb-1 bg-white">@Model.Group.Name</div>
        </div>
        <div class="d-flex">
            <div class="w-25 p-1 pr-4 mb-1 bg-secondary text-white text-right"><label asp-for="ImportId"></label></div>
            <div class="w-75 p-1 pl-4 mb-1 bg-white">@Model.ImportId.Value</div>
        </div>
        <hr />
        <form asp-action="Delete">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Delete" class="btn btn-dark" />
        </form>
    </div>
</div>